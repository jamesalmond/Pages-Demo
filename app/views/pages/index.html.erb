<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    <% @pages.each do |page| -%>
      <tr>
        <td><%= page.name -%></td>
        <td><%= link_to "Edit", edit_page_path(page), :class => 'btn'%></td>
      </tr>
    <% end -%>
  </tbody>
</table>
<%= link_to "New Page", new_page_path, :class => "btn" -%>
